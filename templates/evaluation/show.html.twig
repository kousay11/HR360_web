<!DOCTYPE html>

<title>Evaluation</title>

{% block body %}
    <h1>Evaluation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdEvaluation</th>
                <td>{{ evaluation.idEvaluation }}</td>
            </tr>
            <tr>
                <th>TitreEva</th>
                <td>{{ evaluation.titreEva }}</td>
            </tr>
            <tr>
                <th>NoteTechnique</th>
                <td>{{ evaluation.noteTechnique }}</td>
            </tr>
            <tr>
                <th>NoteSoftSkills</th>
                <td>{{ evaluation.noteSoftSkills }}</td>
            </tr>
            <tr>
                <th>Commentaire</th>
                <td>{{ evaluation.commentaire }}</td>
            </tr>
            <tr>
                <th>DateEvaluation</th>
                <td>{{ evaluation.dateEvaluation ? evaluation.dateEvaluation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Scorequiz</th>
                <td>{{ evaluation.scorequiz }}</td>
            </tr>
            <tr>
                <th>Questions</th>
                <td>{{ evaluation.questions }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_evaluation_index') }}">back to list</a>

    <a href="{{ path('app_evaluation_edit', {'idEvaluation': evaluation.idEvaluation}) }}">edit</a>

    {{ include('evaluation/_delete_form.html.twig') }}
{% endblock %}
